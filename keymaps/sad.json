{
  "atom-workspace atom-text-editor:not(.mini)[data-grammar~=\"sad\"]": {
    "ctrl-;": "sad:draw",
    "ctrl-1": "sad:draw1",
    "ctrl-2": "sad:draw2",
    "ctrl-3": "sad:draw3",
    "ctrl-4": "sad:draw4",
    "ctrl-5": "sad:draw5",
    "ctrl-6": "sad:draw6",
    "ctrl-7": "sad:draw7",
    "ctrl-8": "sad:draw8",
    "ctrl-9": "sad:draw9",
    "ctrl-0": "sad:draw10",
    "ctrl-[": "sad:outdent",
    "ctrl-]": "sad:indent",
    "ctrl-z": "sad:undo",
    "ctrl-y": "sad:redo"
  }
}
